{
  "name": "spot-help",
  "description": "Spot by NetApp Docs",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/spotinst/help"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Spot by NetApp",
    "url": "https://spot.io"
  },
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack -w",
    "start": "concurrently --kill-others-on-fail \"npm:format\" \"npm:build:watch\" \"npm:serve\"",
    "serve": "webpack-dev-server",
    "lint": "eslint --ext .js,.ts,.md,.html ./src",
    "format": "prettier --write 'src/{**/,}*.{html,json,js,pcss,md,yaml}'",
    "test": "HTTP_PORT=$(($$%5000+3000)) codeceptjs run --reporter mocha-multi",
    "test:only": "E2E_SKIP_BOOTSTRAP=true yarn test"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "allure-commandline": "^2.9.0",
    "autoprefixer": "^9.8.6",
    "clean-webpack-plugin": "1.0.1",
    "codeceptjs": "^2.0.7",
    "commitizen": "^4.1.2",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^10.2.11",
    "markdownlint": "^0.16.0",
    "markdownlint-cli": "^0.14.0",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.3",
    "prettier": "^2.0.5",
    "style-loader": "^0.23.1",
    "typescript": "^3.9.7",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.0",
    "yarn": "^1.22.0"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "docsify-cli": "^4.4.1",
    "jquery": "^3.5.0",
    "jquery-ui": "^1.12.1"
  }
}
