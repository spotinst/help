{
  "name": "help",
  "description": "Spot Docs",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/spotinst/help"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Spot by NetApp",
    "url": "https://spot.io"
  },
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack -w",
    "start": "concurrently --kill-others-on-fail \"npm:format\" \"npm:build:watch\" \"npm:serve\"",
    "serve": "webpack serve --mode development --env development",
    "lint": "eslint --ext .js,.ts,.md,.html ./src",
    "format": "prettier --write 'src/{**/,}*.{html,json,js,pcss,md,yaml}'",
    "sitemap": "src/scripts/sitemap.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "docsify-cli": "^4.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^10.3.7",
    "clean-webpack-plugin": "4.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "markdownlint": "^0.24.0",
    "markdownlint-cli": "^0.29.0",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-mixins": "^8.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "sitemap": "^7.0.0",
    "style-loader": "^3.3.0",
    "typescript": "^4.4.3",
    "webpack": "^5.56.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "xml-formatter": "^2.4.1",
    "yarn": "^1.22.15"
  },
  "engines": {
    "node": ">=12"
  }
}
